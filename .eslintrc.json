{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": ["babel", "import"],
  "globals": {
    "fetch": false
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": [2, "never"],
    "arrow-body-style": 0,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0,
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 1,
    "react/require-extension": 0,
    "import/extensions": 1
  }
}
